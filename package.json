{
    "name": "manageemployee",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap/cds": "^6",
        "@sap/xssec": "^3.2.0",
        "express": "^4",
        "hdb": "^0.19.0",
        "passport": "^0.4.1"
    },
    "devDependencies": {
        "@sap/eslint-plugin-ui5-jsdocs": "^2.0.5",
        "@sap/ux-specification": "^1.108.9",
        "@sapui5/ts-types": "^1.92.2",
        "eslint": "^7.32.0",
        "rimraf": "^3.0.2",
        "sqlite3": "^5"
    },
    "scripts": {
        "start": "cds-serve",
        "watch-employees": "cds watch --open employees/webapp/index.html?sap-ui-xx-viewCache=false",
        "undeploy": "cf undeploy manageemployee --delete-services --delete-service-keys --delete-service-brokers",
        "build": "rimraf resources mta_archives && mbt build --mtar archive",
        "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
        "watch-departments": "cds watch --open departments/webapp/index.html?sap-ui-xx-viewCache=false"
    },
    "cds": {
        "requires": {
            "db": "hana-cloud"
        }
    },
    "sapux": [
        "app/departments"
    ]
}